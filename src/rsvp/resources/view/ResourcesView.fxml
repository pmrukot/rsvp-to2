<?import javafx.scene.control.TabPane?>

<?import javafx.scene.control.Tab?>
<TabPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="rsvp.resources.controller.ResourcesController">
    <tabs>
        <Tab text="Rooms List">
            <content>
                <fx:include fx:id="universityRoomListView" source="UniversityRoomListView.fxml"/>
            </content>
        </Tab>
        <Tab text="Time Slots">
            <content>
                <fx:include fx:id="timeSlotListView" source="TimeSlotListView.fxml"/>
            </content>
        </Tab>
    </tabs>
</TabPane>
