<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rsvp.user.controller.AdminController" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="400.0" BorderPane.alignment="CENTER">
    <Button text="Create single user" onAction="#createSingleUSer"/>
    <ComboBox fx:id="userComboBox" promptText="Select user..." onShowing="#loadUsers" visibleRowCount="15"/>
    <Button text="Edit selected user" onAction="#editUser"/>
    <Button text="Delete selected user" onAction="#deleteUser"/>
    <Label text="File"/>
    <Button text="Upload" onAction="#upload"/>
</VBox>