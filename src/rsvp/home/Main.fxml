<?import javafx.scene.control.TabPane?>

<?import javafx.scene.control.Tab?>
<TabPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rsvp.home.AppController">
    <tabs>
        <Tab text="User">
            <content>
                <fx:include fx:id="userPage" source="../user/view/MyCalendarView.fxml"/>
            </content>
        </Tab>
        <Tab text="Resources">
            <content>
                <fx:include fx:id="resourcesPage" source="../resources/view/ClassListView.fxml"/>
            </content>
        </Tab>
        <Tab text="Booking">
            <content>
                <fx:include fx:id="bookingPage" source="../booking/view/BookingOverviewPane.fxml"/>
            </content>
        </Tab>
    </tabs>
</TabPane>
